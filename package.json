{
  "name": "hexcity",
  "version": "0.0.0",
  "description": "Browser-based client to play hexcity (standalone or with Client-Group Server)",
  "browser": "true",
  "scripts": {
    "uncache": "rm -rf .angular/cache",
    "starturl": "http://localhost:4202/?ghost=game7&ext=Event,Event2",
    "make-proto": "bin/make-patch",
    "clean": "rm -rf dist/*",
    "prod": "ng build citymap --configuration production",
    "build": "npm run clean; npm run make-proto; npm run tsc",
    "ng": "ng",
    "newlib": "npm i ../../ts/createjs-lib/thegraid-easeljs-lib-*.tgz ; rm -rf .angular/cache",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@thegraid/common-lib": "^1.3.40",
    "@thegraid/easeljs-lib": "file:../../ts/createjs-lib/thegraid-easeljs-lib-1.3.6.tgz",
    "@thegraid/easeljs-module": "^1.2.2",
    "@thegraid/ezpromise": "^1.2.1",
    "@thegraid/wspbclient": "^1.4.26",
    "google-protobuf": "^3.20.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "~15.2.10",
    "@angular/compiler-cli": "^15.2.0",
    "@types/google-protobuf": "^3.15.5",
    "@types/jasmine": "~4.3.0",
    "@types/wicg-file-system-access": "^2020.9.6",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
}
